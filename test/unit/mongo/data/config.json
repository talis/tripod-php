                        }
                    ]
                }
            }
        },
        {
            "_id": "t_work2",
            "type": "acorn:Work2",
            "from":"CBD_testing",
            "fields": [
                {
                    "fieldName": "type",
                    "predicates": ["rdf:type"]
                }
            ],
            "joins" : {
                "acorn:seeAlso" : {
                    "fields" : [
                        {
                            "fieldName" : "seeAlsoType",
                            "predicates": ["rdf:type"]
                        }
                    ]
                }
            }
        },
        {
            "_id":"t_users",
            "type":"spec:User",
            "from":"CBD_testing",
            "fields":[
                {
                    "fieldName":"lowercase",
                    "predicates":[{
                        "lowercase":{
                            "predicates":["foaf:firstName"]
                        }
                    }]
                },
                {
                    "fieldName":"lowercaseDate",
                    "predicates":[{
                        "lowercase":{
                            "date":{
                                "predicates":["temp:last_login"]
                            }
                        }
                    }]
                },
                {
                    "fieldName":"join",
                    "predicates":[{
                        "join":{
                            "glue":" ",
                            "predicates":["foaf:firstName", "foaf:surname"]
                        }
                    }]
                },
                {
                    "fieldName":"joinSingle",
                    "predicates":[{
                        "join":{
                            "glue":" ",
                            "predicates":["foaf:firstName"]
                        }
                    }]
                },
                {
                    "fieldName":"joinLowerCase",
                    "predicates":[{
                        "lowercase":{
                            "join":{
                                "glue":" ",
                                "predicates":["foaf:firstName", "foaf:surname"]
                            }
                        }
                    }]
                },
                {
                    "fieldName":"joinLowerCaseANDExtraField",
                    "predicates":[
                        {
                            "lowercase":{
                                "join":{
                                    "glue":" ",
                                    "predicates":["foaf:firstName", "foaf:surname"]
                                }
                            }

                        }, "foaf:firstName"]
                },
                {
                    "fieldName":"mongoDate",
                    "predicates":[{
                        "date":{
                            "predicates":["temp:last_login"]
                        }
                    }]
                },
                {
                    "fieldName":"mongoDateInvalid",
                    "predicates":[{
                        "date":{
                            "predicates":["temp:last_login_invalid"]
                        }
                    }]
                },
                {
                    "fieldName":"mongoDateDoesNotExist",
                    "predicates":[{
                        "date":{
                            "predicates":["temp:last_login_DOES_NOT_EXIST"]
                        }
                    }]
                },
                {
                    "fieldName":"userProfileLink",
                    "value": "_link_"
                }
            ]
        },
        {
            "_id": "t_join_link",
            "type":"bibo:Document",
            "from":"CBD_testing",
            "to":"tripod_php_testing_alt",
            "fields" : [
                {
                    "fieldName":"bookLink",
                    "value": "_link_"
                },
                {
                    "fieldName":"authorUri",
                    "predicates":["dct:creator"]
                },
                {
                    "fieldName" : "workUri",
                    "predicates" : ["dct:isVersionOf"]
                }
            ],
            "joins" : {
                "dct:creator" : {
                    "fields" : [
                        {
                            "fieldName":"authorLink",
                            "value" : "_link_"
                        },
                        {
                            "fieldName":"knowsUri",
                            "predicates":["foaf:knows"]
                        }
                    ],
                    "joins" : {
                        "foaf:knows" : {
                            "fields" : [
                                {
                                    "fieldName" : "knowsLink",
                                    "value" : "_link_"
                                }
                            ]
                        }
                    }
                },
                "dct:isVersionOf" : {
                    "fields" :[
                        {
                            "fieldName" : "workLink",
                            "value" : "_link_"
                        }
                    ]
                }
            }
        },
        {
            "_id": "t_link_multiple",
            "type":"bibo:Document",
            "from":"CBD_testing",
            "joins" : {
                "dct:contributor" : {
                    "fields" : [
                        {
                            "fieldName":"contributorLink",
                            "value" : "_link_"
                        }
                    ]
                }
            }
        },
        {
            "_id": "t_distinct",
            "type": ["bibo:Book","bibo:Document"],
            "from": "CBD_testing",
            "fields": [
                {
                    "fieldName": "title",
                    "predicates": ["dct:title",
                        {"join":{
                            "glue":": ",
                            "predicates":["searchterms:title"]
                            }
                        }
                    ]
                },
                {
                    "fieldName": "type",
                    "predicates": ["rdf:type"]
                }
            ]
        }
