                        {
                            "fieldName":"isbn13",
                            "predicates":["bibo:isbn13"]
                        }
                    ]
                }
            },
            "counts" : [
                {
                    "fieldName" : "source_count",
                    "property" : "dct:isVersionOf"
                },
                {
                    "fieldName" : "regex_source_count",
                    "property" : "dct:isVersionOf",
                    "regex" : "/foobar/"
                }
            ]
        },
        {
            "_id": "t_join_source_count_regex",
            "type":"acorn:Resource",
            "from":"CBD_testing",
            "joins" : {
                "acorn:jacsUri" : {
                    "counts": [
                        {
                            "fieldName":"titles_count",
